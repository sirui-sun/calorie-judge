<h1>Listing results</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Gender</th>
    <th>Age</th>
    <th>Result</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @results.each do |result| %>
  <tr>
    <td><%= result.name %></td>
    <td><%= result.gender %></td>
    <td><%= result.age %></td>
    <td><%= result.result %></td>
  </tr>
<% end %>
</table>

